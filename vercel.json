{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "save-users.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/save-users.php",
      "dest": "/save-users.php"
    }
  ],
  "functions": {
    "save-users.php": {
      "runtime": "php@8.0"
    }
  }
}
